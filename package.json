{
  "name": "node-js-getting-started",
  "version": "0.2.5",
  "description": "A sample Node.js app using Express 4",
  "engines": {
    "node": "5.9.1"
  },
  "main": "index.js",
  "scripts": {
    "webpack-dev-server": "webpack-dev-server --hot --inline --history-api-fallback ",
    "express-server": "nodemon server.js",
    "dev": "concurrent --kill-others \"npm run webpack-dev-server\" \"npm run express-server\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "postinstall": "webpack -p --config webpack.production.js"
  },
  "dependencies": {
    "appcache-webpack-plugin": "^1.3.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.9.2",
    "body-parser": "^1.15.2",
    "cors": "^2.7.1",
    "css-loader": "^0.23.1",
    "express": "4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "knex": "^0.11.7",
    "node-uuid": "^1.4.7",
    "pg": "^6.0.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-mdl": "^1.6.1",
    "react-redux": "^4.4.5",
